# and given that they do not seem to actually be used anywhere I would
# prefer to skip handling them as well.
feature test {
    valueRecordDef 123 BAD;
} test;

lookup shmookup {
    valueRecordDef <4 3 2 1> ALSO_BAD;
} shmookup;
